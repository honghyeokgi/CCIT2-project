        <div class="funkyradio-default">
            <input type="radio"  value="10" name="time" id="radio1" />
            <label for="radio1">10초 동안 작동</label>
        </div>
        <div class="funkyradio-primary">
            <input type="radio" value="20" name="time" id="radio2" checked/>
            <label for="radio2" >20초 동안 작동 </label>
        </div>
        <div class="funkyradio-success">
            <input type="radio" value="40" name="time" id="radio3" />
            <label for="radio3">40초 동안 작동</label>
        </div>
        <div class="funkyradio-danger">
            <input type="radio" value="1" name="time" id="radio4" />
            <label for="radio4">1분 동안 작동</label>
        <div class="funkyradio-info">
            <input type="radio" value="0" name="time" id="radio6" />
            <label for="radio6">종료하기</label>
        </div>
      </div>
      <div class="funkyradio-info">

